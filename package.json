{
  "name": "saas-boilerplate",
  "version": "1.7.7",
  "scripts": {
    "dev": "lsof -ti:3000 | xargs -r kill -9; next dev",
    "build": "next build",
    "start": "next start",
    "build:analyze": "cross-env ANALYZE=true next build",
    "clean": "rimraf .next out coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "bunx --bun vitest run",
    "test:watch": "bunx --bun vitest",
    "test:update": "bunx --bun vitest run -u",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "prepare": "husky",
    "test:unit": "bunx --bun vitest run --coverage",
    "test:integration": "bunx --bun vitest run --config vitest.integration.config.ts",
    "test:auth": "bunx --bun vitest run -t '(auth|session|login)'",
    "test:security": "bunx --bun vitest run -t 'security'",
    "test:e2e:auth": "playwright test tests/auth",
    "test:all": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
    "test:smoke": "playwright test --grep '@smoke'",
    "db:seed": "tsx scripts/seed.ts",
    "db:seed:test": "NODE_ENV=test tsx scripts/seed.test.ts",
    "security:audit": "pnpm audit && eslint . --ext .ts,.tsx --plugin security",
    "auth:test-email": "tsx scripts/test-email.ts",
    "trpc:check": "tsc --noEmit --project tsconfig.trpc.json"
  },
